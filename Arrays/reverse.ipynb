   "source": [
    "# Reversing an array\n",
    "\n",
    "arr = [4, 6, 9, 3]\n",
    "\n",
    "start = 0\n",
    "end = len(arr) - 1\n",
    "\n",
    "while start < end:\n",
    "    arr[start], arr[end] = arr[end], arr[start]\n",
    "    start += 1\n",
    "    end -= 1\n",
    "\n",
    "print(arr)"
   ]
